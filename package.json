    "build": "webpack --mode production",
    "build:server": "babel ./server/ -d ./dist",
    "int:test": "export NODE_ENV=test&&jest tests --detectOpenHandles --forceExit",
    "unit:test": "export NODE_ENV=test&&jest src/**/test/*.spec.js --detectOpenHandles --forceExit",
    "int:test:win": "SET NODE_ENV=test&&jest tests --detectOpenHandles --forceExit",
    "unit:test:win": "SET NODE_ENV=test&&jest src/**/test/*.spec.js --detectOpenHandles --forceExit",
    "test": "npm run int:test && npm run unit:test",
    "test:win": "npm run int:test:win && npm run unit:test:win"
